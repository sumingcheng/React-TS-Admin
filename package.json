{
  "name": "reactantworks",
  "version": "1.0.0",
  "description": "React+webpack+ant design+axios+less",
  "main": "./src/main.tsx",
  "sideEffects": false,
  "scripts": {
    "dev": "cross-env APP_ENV=development webpack serve",
    "prod": "cross-env APP_ENV=production webpack serve",
    "test": "cross-env APP_ENV=test webpack serve",
    "sit": "cross-env APP_ENV=sit webpack serve",
    "build": "npm run clean && cross-env APP_ENV=development webpack",
    "build:prod": "npm run clean && cross-env APP_ENV=production  webpack",
    "build:test": "npm run clean && cross-env APP_ENV=test webpack",
    "build:sit": "npm run clean && cross-env APP_ENV=sit webpack",
    "clean": "bash ./bin/build.sh",
    "postbuild": "node ./bin/zip.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [],
  "author": "sumingcheng",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.7.3",
    "archiver": "^5.3.1",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "redux": "^4.2.1",
    "terser-webpack-plugin": "^5.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^4.3.8",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "postcss": "^8.4.27",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.13.3"
  }
